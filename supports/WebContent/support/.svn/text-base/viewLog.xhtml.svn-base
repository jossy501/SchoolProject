<!DOCTYPE composition PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN"
    "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
                xmlns:s="http://jboss.com/products/seam/taglib"
                xmlns:ui="http://java.sun.com/jsf/facelets"
                xmlns:f="http://java.sun.com/jsf/core"
                xmlns:h="http://java.sun.com/jsf/html"
                xmlns:rich="http://richfaces.org/rich"
                xmlns:a4j="http://richfaces.org/a4j"
                template="../WEB-INF/layout/basic_layout.xhtml">
                
<ui:define name="body">

	<!-- processing panel -->
    
    <rich:modalPanel id="panelp" width="150" height="50">
       <h:form id="pFrm">
        <h:panelGrid columns="1" rowClasses="prop" columnClasses="name2,value">
           <h:outputText value="Please wait...." styleClass="messagesingle" align="center">
               <h:graphicImage value="/images/loading_small.gif"/>
           </h:outputText>
          </h:panelGrid>  
         </h:form>
    </rich:modalPanel>

    <h:form id="manageCardFrm">
        <h:inputHidden value="#{reportAction.mobileno}" id="hiddenId" />

    	<a4j:form>
	        <a4j:commandButton id="submit"
	                           value="    Back   " onclick="history.back()">
	        </a4j:commandButton>
    	</a4j:form>
        
        <rich:panel style="width:100%">

            <f:facet name="header">DATA FROM SMS RECEIVE</f:facet>

            <rich:dataTable id="rcardTb" rows="20" value="#{reportAction.smsReceiveLog}"  var="crd">
                
                
                <rich:column>
                    <f:facet name="header">Sms Key</f:facet>
            		<h:outputText value="#{crd.sms_key}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Destination</f:facet>
                    <h:outputText value="#{crd.sms_dest}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Originator</f:facet>
                    <h:outputText value="#{crd.sms_originator}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Status</f:facet>
                    <h:outputText value="#{crd.sms_status}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Message</f:facet>
                    <h:outputText value="#{crd.sms_message}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Trans Date</f:facet>
                    <h:outputText value="#{crd.trans_dt}"/>
                </rich:column>
                
            </rich:dataTable>

        </rich:panel>
        
        
        <rich:spacer width="10px" height="30px"/>
        
        <rich:panel style="width:100%">

            <f:facet name="header">DATA FROM SMS LOG</f:facet>
            

            <rich:dataTable id="smsLogTb" rows="20" value="#{reportAction.smsLog}"  var="crd">
                
                <rich:column>
                    <f:facet name="header">Unique TransID</f:facet>
                    <h:outputText value="#{crd.unique_transid}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Mobile</f:facet>
                    <h:outputText value="#{crd.mobile}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Trans Type</f:facet>
                    <h:outputText value="#{crd.trans_type}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Response Message</f:facet>
                    <h:outputText value="#{crd.response_message}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Response Code</f:facet>
                    <h:outputText value="#{crd.response_code}"/>
                </rich:column>
                
                
                <rich:column>
                    <f:facet name="header">Date</f:facet>
                    <h:outputText value="#{crd.message_date}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Delay Time(In secs)</f:facet>
                    <h:outputText value="#{crd.response_time_in_secs}"/>
                </rich:column>

            </rich:dataTable>

        </rich:panel>
        
        <rich:spacer width="10px" height="30px"/>
        
        <rich:panel style="width:100%">

            <f:facet name="header">DATA FROM REQUEST LOG</f:facet>

            <rich:dataTable id="reqTb" rows="20" value="#{reportAction.failedFundsLog}"  var="crd">
                
                <rich:column>
                    <f:facet name="header">Trans ID</f:facet>
                    <h:outputText value="#{crd.transid}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Card Number</f:facet>
                    <h:outputText value="#{crd.card_num}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Merchant</f:facet>
                    <h:outputText value="#{crd.merchant_desc}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Trans Code</f:facet>
                    <h:outputText value="#{crd.trans_code}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Trans Descr</f:facet>
                    <h:outputText value="#{crd.trans_descr}"/>
                </rich:column>
                
                
                <rich:column>
                    <f:facet name="header">Response Code</f:facet>
                    <h:outputText value="#{crd.response_code}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Delay Time(In secs)</f:facet>
                    <h:outputText value="#{crd.response_time_in_secs}"/>
                </rich:column>
                

            </rich:dataTable>

        </rich:panel>
        
        <rich:spacer width="10px" height="30px"/>
        
        <rich:panel style="width:100%">

            <f:facet name="header">DATA FROM SMS SEND</f:facet>

            <rich:dataTable id="smsSendTb" rows="20" value="#{reportAction.smsSendLog}"  var="crd">
                
                <rich:column>
                    <f:facet name="header">Sms Key</f:facet>
                    <h:outputText value="#{crd.sms_key}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Destination</f:facet>
                    <h:outputText value="#{crd.sms_dest}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Originator</f:facet>
                    <h:outputText value="#{crd.sms_originator}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Status</f:facet>
                    <h:outputText value="#{crd.sms_status}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Sms Message</f:facet>
                    <h:outputText value="#{crd.sms_message}"/>
                </rich:column>
                
                
                <rich:column>
                    <f:facet name="header">Sms Date</f:facet>
                    <h:outputText value="#{crd.sms_date}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Delay Time(In secs)</f:facet>
                    <h:outputText value="#{crd.response_time_in_secs}"/>
                </rich:column>
                
            </rich:dataTable>

        </rich:panel>
        
        
        <rich:spacer width="10px" height="30px"/>
        
        <rich:panel style="width:100%">

            <f:facet name="header">DATA FROM VTU LOG</f:facet>

            <rich:dataTable id="vtuTb" rows="20" value="#{reportAction.vtuLog}"  var="crd">
                
                
                <rich:column>
                    <f:facet name="header">ID</f:facet>
                    <h:outputText value="#{crd.id}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Trans ID</f:facet>
                    <h:outputText value="#{crd.unique_trans_id}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Line Type</f:facet>
                    <h:outputText value="#{crd.line_type}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Voucher Type</f:facet>
                    <h:outputText value="#{crd.voucher_type}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Destination</f:facet>
                    <h:outputText value="#{crd.dest_msisdn}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Status</f:facet>
                    <h:outputText value="#{crd.status_id}"/>
                </rich:column>
                
                
                <rich:column>
                    <f:facet name="header">Response Message</f:facet>
                    <h:outputText value="#{crd.response_message}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Amount</f:facet>
                    <h:outputText value="#{crd.amount}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Date</f:facet>
                    <h:outputText value="#{crd.txn_date_time}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Response Date</f:facet>
                    <h:outputText value="#{crd.response_date}"/>
                </rich:column>
                
            </rich:dataTable>

        </rich:panel>
        
        <rich:spacer width="10px" height="30px"/>
        
        
        <rich:panel style="width:100%">

            <f:facet name="header">DATA FROM E TRANSACTION LOG</f:facet>

            <rich:dataTable id="eTranTb" rows="20" value="#{reportAction.tranLog}"  var="crd">

               
                <rich:column>
                    <f:facet name="header">Trans ID</f:facet>
                    <h:outputText value="#{crd.transid}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Card Number</f:facet>
                    <h:outputText value="#{crd.card_num}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Desc</f:facet>
                    <h:outputText value="#{crd.trans_desc}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Merchant Code</f:facet>
                    <h:outputText value="#{crd.merchat_code}"/>
                </rich:column>
                
                <rich:column>
                    <f:facet name="header">Amount</f:facet>
                    <h:outputText value="#{crd.trans_amount}"/>
                </rich:column>
                
                
                <rich:column>
                    <f:facet name="header">Date</f:facet>
                    <h:outputText value="#{crd.trans_date}"/>
                </rich:column>

            </rich:dataTable>

        </rich:panel>
        
    </h:form>
    
</ui:define>
</ui:composition>
