package com.etranzact.supportmanager.dto;

import java.util.ArrayList;

/**
 * @author tony.ezeanya
 *
 */
public class ProfileMenuOptions 
{

	private String menu_nm;
	private MenuItem menuitem;
	private ArrayList arr_menuitems;
	
	public ProfileMenuOptions(){}
	
	public ProfileMenuOptions(String menu_nm, ArrayList arr_menuitems)
	{
		this.menu_nm = menu_nm;
		this.arr_menuitems = arr_menuitems;
	}

	public String getMenu_nm() {
		return menu_nm;
	}

	public void setMenu_nm(String menu_nm) {
		this.menu_nm = menu_nm;
	}

	public MenuItem getMenuitem() {
		return menuitem;
	}

	public void setMenuitem(MenuItem menuitem) {
		this.menuitem = menuitem;
	}

	public ArrayList getArr_menuitems() {
		return arr_menuitems;
	}

	public void setArr_menuitems(ArrayList arr_menuitems) {
		this.arr_menuitems = arr_menuitems;
	}
	
	
	
	
}
